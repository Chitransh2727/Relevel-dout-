const matrix = [
    [12, 0, 0],
    [0, 25, 0],
    [0, 0, 36]
];
function checkdigonal(matrix) {
    for (let i = 0; i < matrix.length; i++){

for (let j = 0 ; j < matrix[i].length ; j++) {
if(i!==j && matrix[i][j]!=0) return true;
}
    }
    return false;
}
console.log(checkdigonal(matrix));

result------true // matrix is digonal
