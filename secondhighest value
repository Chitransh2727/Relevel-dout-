question 2 :- /**
 * Q - 2 write a function to
 * return all the values in array which is divisible by 7
 // first declare the array 
 // then we take a varriable  where we want to save our ans
// than impliment the logic 
 

 let newarray = [35, 14, 36, 48, 26]
let divby7 = []
for (let i = 0; i <= newarray.length - 1; i++) {
    if (newarray[i] % 7 == 0) {
        divby7.push(newarray[i]);
    }

}
console.log(divby7);
